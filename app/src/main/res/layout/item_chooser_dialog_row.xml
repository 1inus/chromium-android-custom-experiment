<RelativeLayout android:background="@drawable/item_chooser_row_background" android:layout_height="48dp" android:layout_width="match_parent" android:paddingLeft="16dp" android:paddingRight="16dp" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
  
    
  <!-- contentDescription is added at runtime. -->
  
    
  <ImageView android:id="@+id/icon" android:layout_centerVertical="true" android:layout_height="24dp" android:layout_marginRight="16dp" android:layout_width="24dp" tools:ignore="ContentDescription"/>
  
    
  <TextView android:ellipsize="end" android:id="@+id/description" android:layout_centerVertical="true" android:layout_height="wrap_content" android:layout_toRightOf="@id/icon" android:layout_width="match_parent" android:maxLines="1" android:textColor="@color/item_chooser_row_text_color" android:textSize="16sp"/>
  

</RelativeLayout>
