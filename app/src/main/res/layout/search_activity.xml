<FrameLayout android:id="@+id/control_container" android:layout_height="match_parent" android:layout_width="match_parent" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
  

    
  <ViewStub android:background="@android:color/white" android:id="@+id/omnibox_results_container_stub" android:layout="@layout/omnibox_results_container" android:layout_height="wrap_content" android:layout_width="match_parent"/>
  

    
  <ImageView android:id="@+id/toolbar_shadow" android:layout_height="wrap_content" android:layout_marginTop="@dimen/toolbar_height_no_shadow" android:layout_width="match_parent" android:scaleType="fitXY" android:src="@drawable/toolbar_shadow" tools:ignore="ContentDescription"/>
  

    
  <FrameLayout android:background="@android:color/white" android:clickable="true" android:id="@+id/toolbar" android:layout_height="@dimen/toolbar_height_no_shadow" android:layout_width="match_parent">
    

        
    <org.chromium.chrome.browser.searchwidget.SearchActivityLocationBarLayout android:id="@+id/search_location_bar" android:layout_gravity="center_vertical" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/search_activity_location_bar_margin_start" android:layout_marginRight="@dimen/search_activity_location_bar_margin_end" android:layout_width="match_parent"/>
    

    
  </FrameLayout>
  

    
  <FrameLayout android:id="@+id/bottom_container" android:layout_height="match_parent" android:layout_width="match_parent"/>
  


</FrameLayout>
